<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nethuyahampath.nakathapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


        <activity android:name=".nakathListActivity"/>



        <activity
            android:name=".MoonActivity"
            android:label="@string/title_activity_moon"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".bathActivity"
            android:label="@string/title_activity_bath"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sunActivity"
            android:label="@string/title_activity_sun"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>